algoritmo "semnome"
var
    C, N, ContDiv: Inteiro
inicio
      C <- 1
      ContDiv <- 0
      Escreva ("Digite um numero: ")
      Leia (N)
      Repita
            se (N % C = 0) entao
               ContDiv <- ContDiv + 1
            FimSe
            C <- C+ 1
      Ate (C > N)
      Se (ContDiv > 2) entao
         Escreval ("O avlor de", N, " nao e primo")
      Senao
           Escreval ("O valor de", N, " e primo")
      FimSe

fimalgoritmo